

<h1>Welcome Traveler!</h1>

<div class='form-container'>

<% if flash[:errors] %>
    <p style="color: red;"><%= flash[:errors]%></p>
<% end %>

<%= form_tag "/sessions" do %>
    <%= label_tag "Username: " %>
    <%= text_field_tag :username %><br>

    <%= label_tag "Password: " %>
    <%= password_field_tag :password %>

    <%= submit_tag "Login" %>
<% end %>
<div class='form-container'>

